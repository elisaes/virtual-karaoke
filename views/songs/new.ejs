<div class="container">
    <% songs.forEach(song=>{%>
    <div><%=song._id%></div>
    <a href="/songs/show/<%=song._id%>" class="list-group-item list-group-item-action">
        <div><%= song.title%> by <%= song.artist%></div>
    </a>
    <form action="/songs/<%=song._id%>?_method=DELETE" enctype="multipart/form-data" method="POST">
        <input type="submit" value="Delete Song" class="btn btn-primary btn-block" />

    <% })%>
</form>

    <div class="row">
        <!-- in form action put the route -->


        <form action="/songs/new" enctype="multipart/form-data" method="POST">
            <div>
                <label>Title:</label>
                <input type="text" name="title" class="form-control" />
            </div>
            <div>
                <label>Artist:</label>
                <input type="text" name="artist" class="form-control" />
            </div>
            <div>
                <label>Clip:</label>
                <input type="file" name="music" class="form-control" />

            </div>
            <div>
                <label>Lyrics:</label>
                <input type="file" name="lyric" class="form-control" />

            </div>
            <div class="mt-3">
                <input type="submit" value="Upload" class="btn btn-primary btn-block" />
            </div>
        </form>
    </div>

